{
  "expo": {
    "name": "kp0s",
    "slug": "kp0s",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kp0s",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kushcedelicorg.kp0s",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "k.p0s perlu mo akses Bluetooth for mo menghubung dg bacetak struk ka printer.",
        "NSBluetoothPeripheralUsageDescription": "k.p0s perlu mo akses Bluetooth for mo menghubung dg bacetak struk ka printer."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3D3115",
        "foregroundImage": "./assets/images/android-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kushcedelicorg.kp0s",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "suppressSafeAreaViewWarning": true,
      "eas": {
        "projectId": "0ecb3092-c13a-480d-ade3-6e5440d154fa"
      }
    },
    "owner": "kushcedelic-org",
    "updates": {
      "url": "https://u.expo.dev/0ecb3092-c13a-480d-ade3-6e5440d154fa"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
